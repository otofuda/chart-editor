{
  "name": "chart-editor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "rsbuild dev",
    "build": "rsbuild build",
    "preview": "rsbuild preview"
  },
  "dependencies": {
    "buryjs": "https://github.com/mtsgi/bury.git#feature/vite_typescript",
    "chart-types": "^2.0.2",
    "core-js": "^3.32.0",
    "html2canvas": "^1.0.0-rc.7",
    "vue": "2.7",
    "vuetify": "^2.6.0"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.5.11",
    "@rsbuild/plugin-sass": "^1.4.0",
    "@rsbuild/plugin-vue2": "^1.0.4",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "sass": "^1.64.2",
    "sass-loader": "^10.0.0",
    "typescript": "~4.5.5",
    "vue-template-compiler": "2.6.11",
    "vuetify-loader": "^1.7.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": [
        "error"
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "volta": {
    "node": "22.19.0",
    "npm": "10.9.3"
  }
}
